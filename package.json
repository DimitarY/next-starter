{
  "name": "next-starter",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.x",
    "pnpm": "10.x"
  },
  "packageManager": "pnpm@10.15.1",
  "scripts": {
    "dev": "dotenv -e .env.development -- sh -c 'next dev --turbopack --experimental-https ${LOCAL_DNS_RECORD:+--hostname $LOCAL_DNS_RECORD}'",
    "dev:email": "cp .env ./node_modules/react-email/dist/preview && email dev --port 5000 --dir src/components/emails",
    "build": "next build",
    "build:container": "cross-env SKIP_ENV_VALIDATION=true next build",
    "start": "next start",
    "start:standalone": "node .next/standalone/server.js",
    "lint": "biome lint --write",
    "format": "biome format --write",
    "db:generate": "drizzle-kit generate",
    "db:drop": "drizzle-kit drop",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
