{
  "name": "next-starter",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.x",
    "pnpm": "10.x"
  },
  "packageManager": "pnpm@10.5.0",
  "scripts": {
    "dev": "dotenv -e .env.development -- sh -c 'next dev --turbopack --experimental-https ${LOCAL_DNS_RECORD:+--hostname $LOCAL_DNS_RECORD}'",
    "dev:email": "cp .env ./node_modules/react-email/dist/preview && email dev --port 5000 --dir src/components/emails",
    "build": "next build",
    "build:container": "cross-env SKIP_ENV_VALIDATION=true next build",
    "start": "next start",
    "start:standalone": "node .next/standalone/server.js",
    "lint": "next lint",
    "format": "prettier --write .",
    "db:generate": "drizzle-kit generate",
    "db:drop": "drizzle-kit drop",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.2",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@react-email/components": "^0.0.33",
    "@react-email/render": "^1.0.5",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tanstack/react-query": "^5.66.9",
    "@uploadthing/react": "^7.3.0",
    "better-auth": "^1.1.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.39.3",
    "framer-motion": "^12.4.7",
    "lucide-react": "^0.476.0",
    "next": "^15.1.7",
    "next-themes": "^0.4.4",
    "postgres": "^3.4.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "react-easy-crop": "^5.4.0",
    "react-hook-form": "^7.54.2",
    "react-icons": "^5.5.0",
    "react-image-crop": "^11.0.7",
    "resend": "^4.1.2",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "uploadthing": "^7.5.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.66.1",
    "@tanstack/react-query-devtools": "^5.66.9",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^22.13.5",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "^0.30.4",
    "eslint": "^9.21.0",
    "eslint-config-next": "^15.1.7",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-drizzle": "^0.2.3",
    "postcss": "^8.5.3",
    "prettier": "^3.5.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react-email": "^3.0.7",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3"
  }
